{
  "name": "proteus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "api-types:remove-prefix": "sed --expression \"s/Application\\.Queries\\.//g\" --expression \"s/Domain\\.Entities\\.Models\\.//g\" --in-place src/api/swagger.json",
    "api-types:generate": "openapi-typescript src/api/swagger.json -o src/api/types.ts --root-types --root-types-no-schema-prefix",
    "api-types:fmt": "prettier --write src/api/types.ts",
    "api-types": "pnpm api-types:remove-prefix && pnpm api-types:generate && pnpm api-types:fmt",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "15.3.0",
    "openapi-fetch": "^0.13.5",
    "react": "19.1.0",
    "react-day-picker": "^9.6.4",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.55.0",
    "styled-components": "^6.1.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.17.30",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.1",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.10.0",
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.1",
      "@types/react-dom": "19.1.2"
    },
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
